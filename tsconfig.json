{
  "compilerOptions": {
    "lib": ["es2015", "dom", "dom.iterable", "esnext"],
    "noImplicitAny": false,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "suppressImplicitAnyIndexErrors": true,
    "moduleResolution": "node"
  },
  "include": ["./"]
}
